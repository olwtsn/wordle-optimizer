{
  "rewrites": [
    {
      "source": "/wordle",
      "destination": "/public/wordle.html"
    },
    {
      "source": "/wordle/api/best-word",
      "destination": "/api/wordle/best-word.py"
    },
    {
      "source": "/api/wordle/best-word",
      "destination": "/api/wordle/best-word.py"
    }
  ],
  "functions": {
    "api/wordle/best-word.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
