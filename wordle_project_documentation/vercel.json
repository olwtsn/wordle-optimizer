{
  "version": 2,
  "builds": [
    {
      "src": "api/wordle/best-word.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/wordle/api/best-word",
      "dest": "/api/wordle/best-word.py"
    },
    {
      "src": "/api/wordle/best-word",
      "dest": "/api/wordle/best-word.py"
    },
    {
      "src": "/wordle",
      "dest": "/public/wordle.html"
    },
    {
      "src": "/",
      "dest": "/public/wordle.html",
      "status": 302
    }
  ]
}